	sp2130_standalone: sp2130-charger@6D {
		status = "okay";
        compatible = "nvt,sp2130-standalone";
        reg = <0x6D>;
		sp2130,irq-gpio = <&tlmm 97 0>;
		#io-channel-cells = <1>;

        nvt,sp2130,bat-ovp-disable = <0>;
        nvt,sp2130,bat-ocp_disable = <0>;
