&tlmm{
	sar_int_en: sar__int {
		mux {
			pins = "gpio106";
			function = "gpio";
		};
		config {
			pins = "gpio106";
			drive-strength = <2>;
			bias-pull-up;
		};
	};
};
&tlmm{
        psensor_ldo: psensor {
                mux {
                        pins = "gpio67";
                        function = "gpio";
                };
                config {
                        pins = "gpio67";
                        drive-strength = <2>;
                        bias-pull-up;
                        output-high;
                };
        };
};
